curl https://www.torproject.org/download/ | sed -n 's/.*href="\([^"]*\).*/\1/p' | grep tar.xz | sed -n 1p >u && sed 's,/dist,https://www.torproject.org/dist,g' u | xargs wget && tar -xf tor*.tar.xz && rm u tor*.xz* && mv tor-browser /opt && cd /opt/tor-browser && ./start-tor-browser.desktop --register-app